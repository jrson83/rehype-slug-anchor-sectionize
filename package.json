{
  "name": "rehype-slug-anchor-sectionize",
  "version": "0.2.0",
  "description": "rehype plugin which wraps headings and their descendants in nested <section> elements and applies slugged anchor links to them.",
  "keywords": [
    "anchor",
    "anchorize",
    "ast",
    "hast",
    "heading",
    "markdown",
    "plugin",
    "rehype",
    "rehype-plugin",
    "section",
    "sectionize",
    "slug",
    "slugify",
    "unified"
  ],
  "homepage": "https://github.com/jrson83/rehype-slug-anchor-sectionize#readme",
  "bugs": {
    "url": "https://github.com/jrson83/rehype-slug-anchor-sectionize/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrson83/rehype-slug-anchor-sectionize.git"
  },
  "license": "MIT",
  "author": "JÃ¶rn Spangenberg <just@jrson.me> (https://jrson.me)",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "src"],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf dist",
    "lint": "biome check --apply-unsafe .",
    "fmt": "biome format . --write",
    "build": "tsc",
    "watch": "tsc --watch",
    "type:check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "github-slugger": "^2.0.0",
    "hast-util-to-string": "^3.0.0",
    "unist-util-find-after": "^5.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.2.2",
    "@types/hast": "^3.0.1",
    "@types/node": "^20.7.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "typescript": "^5.2.2",
    "unified": "^11.0.3",
    "unist-util-is": "^6.0.0",
    "vitest": "^0.34.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nano-staged": {
    "*.ts": "biome check --apply-unsafe .",
